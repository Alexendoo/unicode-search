{
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "wasm_bindgen": true
    },
    "rules": {
        "react/prop-types": ["off"],
        "camelcase": ["off"]
    },
    "settings": {
        "import/ignore": [
            "intermediate"
        ]
    }
}
